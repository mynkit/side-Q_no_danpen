  $ sometimesBy 0.8 (|+ n (choose [0..2]))
  $ s "record/2"
  # n 7
  # hpf 100
  # amp 0.4

d2
  $ slow (range 1 4 rand)
  $ sometimesBy 0.09 (|* speed (-1))
  $ s "~ [~cassette] ~ cassette"
  # speed (range 1 0.5 $ slow 3 rand)
  # n (choose [4,6,7,8,9,10])
