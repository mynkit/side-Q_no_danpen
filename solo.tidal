bpm = 90

setcps (bpm/60/4)

all $ (|* amp 1.0)


-- intro

-- 先にSC側でfire

d1
  $ slow 2
  $ degradeBy 0.5
  $ (|- up (choose [0,5,12,4]))
  $ s "coffee"
  # n (choose ([10,11,12]))
  # up "<0,2,4,12>"
  # room 0.9 # sz 0.97 # dry 0
  # resi 1
  # theta (line)
  # amp 0.2

d2
  $ slow 4
  $ (|- up (choose [12..24]))
  $ s "coffee"
  # n (choose ([1]))
  # up "<0,2,4,12>"
  # room 0.9 # sz 0.97 # dry 3
  # theta (line)
  # amp 0.13

d3
  $ slow 4
  $ (|- up (choose [24,20,17]))
  $ s "coffee"
  # n "<13,14>"
  # up "<0,7,4,12>"
  # room 0.9 # sz 0.97 # dry 4
  # theta 0
  # amp 0.1

d7
  $ (|+ n (12))
  $ s "~ superfm/2"
  # n "<e5,a6>"
  # sustain 0.05
  # amp 0.1
  # room 0.5 # sz 0.9
  -- # scReverb 0.8 # ice 0.9
  # hpf 600
  # theta 0 # dis 1

d4
  $ when ((/=3).(flip mod 8)) (degradeBy 1)
  $ whenmod 16 8  (|- n (5))
  $ mix ((|- n 12).(|* amp 0.3))
  $ s "~ ~ [~FastSinSynth]~"
  # n "<e5,a6>"
  # theta (-1)
  # sustain 12
  # resi 1
  # room 0.9 # sz 0.9 # dry 4
  # pingpong 0.7 # pingpongt (3/8) # pingpongfb 0.7
  # amp 0.1

do
  let
    speed = (1 + (0.1 * (5))) * (range 0.4 0.3 rand)
  d5
    $ (|* amp 2)
    $ (|* freq speed)
    $ (|* sustain (1/speed))
    $ (|* accelerate speed)
    $ ((1/4)~>)
    $ someCyclesBy 0.2 (degradeBy 1)
    $ someCyclesBy 0.7 (mix (((choose [(3/32),(1/32)])~>).(|* amp 0.6).(|* freq 2).(|* sustain 0.5)))
    $ s "simpleSineWave/2!4 ~!200"
    # freq "4000 3037 1582 505!201"
    # sustain "0.01 0.02 0.05 0.07!201"
    # accelerate "2.2 3.1 4.9 5!201"
    # amp "0.1 0.2 0.3 0.2!201"
    # theta (0.3 * fast 5 line) # dis 1
    # hpf 2000
    # lpf 7000
    # resi 1
    -- # room 0.3 # sz 0.3

d3
  $ slow 4
  $ s (fit 0 ["liminalbd"] "0 [~ 0*2] ~ 0")
  # amp 0.25
  # hpf 200

d7
  -- $ degradeBy 0.08
  $ s "guitarrr/4"
  # n "<8,10>"
  # room 0.9 # sz 0.7 # dry 3
  # amp 0.1


d1
  $ degradeBy 0.7
  $ (|* amp 1)
  $ s "supergong*8"
  # n "<e4,e5>" 
  # sustain 0.1
  # room 0.3 # sz 0.0
  # theta (range (-0) 0 $ fast 7 rand)
  # amp (range 0.1 0.2 rand)


d2
  $ when ((/=3).(flip mod 4)) (degradeBy 1)
  $ (|- up 12)
  $ s "~ ~ [~guitar_async] ~"
  # n "<0,13,14,15>"
  # room 0.9 # sz 0.7 # dry 2
  # resi 1
  # speed "<1,2>"
  # amp 0.3

d9
  $ slow 2
  $ mix ((|+ n 5).(|* amp 0.6))
  $ s (fit 0 ["padDirt"] "~0 0~ ~~ ~~ ~0/2 ~~ 0/4~ ~~")
  # n "<cs4 e4> <fs5,a4> ~ ~  <e5> ~ <cs3> ~"
  # amp 0.5
  # room 0.8 # sz 0.9
  # resi 1
  # hpf 150
  # pingpong 0.7 # pingpongt (3/16) # pingpongfb 0.7
  # sustain 3

-- architected noise

